{% extends 'DirectokiBundle::base.html.twig' %}

{% block title %}Welcome{% endblock %}

{% block main %}


	<ol class="breadcrumb">
		<li>Project: {{  project.title }}</li>
		<li class="active">Directory: {{  directory.title }}</li>
	</ol>

	<h1>Directory: {{  directory.title }}</h1>



	<a href="{{  path('directoki_project_directory_record_list', { 'projectId':project.publicId,'directoryId':directory.publicId }) }}" class="btn btn-default">Records</a>
	<a href="{{  path('directoki_project_directory_record_list_needing_attention', { 'projectId':project.publicId,'directoryId':directory.publicId }) }}" class="btn btn-default">Records Needing Attention</a>
	<a href="{{  path('directoki_project_directory_fields', { 'projectId':project.publicId,'directoryId':directory.publicId }) }}" class="btn btn-default">Fields</a>

{%  endblock %}
